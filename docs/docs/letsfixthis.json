                    "args": "{\"file_path\": \"src/ui/project_dashboard.py\", \"initial_content\": \"#!/usr/bin/env python3\\n\\\"\\\"\\\"\\nRevolutionary Card Grader - Project Dashboard\\nLightweight project management interface with lazy loading\\n\\\"\\\"\\\"\\n\\nimport customtkinter as ctk\\nimport tkinter as tk\\nfrom tkinter import messagebox, simpledialog\\nfrom pathlib import Path\\nfrom typing import Optional, Callable\\nimport sys\\nsys.path.append(str(Path(__file__).parent.parent))\\nfrom src.core.project_manager import ProjectManager\\nfrom src.ui.revolutionary_theme import RevolutionaryTheme\\n\\nclass ProjectDashboard(ctk.CTkFrame):\\n    \\\"\\\"\\\"\\n    Lightweight project management dashboard\\n    Shows before heavy enterprise systems load\\n    \\\"\\\"\\\"\\n    \\n    def __init__(self, parent, on_project_selected: Optional[Callable] = None):\\n        \\\"\\\"\\\"\\n        Initialize project dashboard\\n        \\n        Args:\\n            parent: Parent widget\\n            on_project_selected: Callback when project is selected\\n        \\\"\\\"\\\"\\n        super().__init__(parent, fg_color=RevolutionaryTheme.QUANTUM_DARK)\\n        \\n        self.on_project_selected = on_project_selected\\n        self.project_manager = ProjectManager()\\n        self.selected_project = None\\n        \\n        self.setup_ui()\\n        self.refresh_projects()\\n    \\n    def setup_ui(self):\\n        \\\"\\\"\\\"Setup the dashboard UI\\\"\\\"\\\"\\n        \\n        # Configure grid\\n        self.grid_columnconfigure(0, weight=1)\\n        self.grid_rowconfigure(1, weight=1)\\n        \\n        # Header\\n        header_frame = ctk.CTkFrame(self, fg_color=RevolutionaryTheme.NEURAL_GRAY, corner_radius=10)\\n        header_frame.grid(row=0, column=0, sticky=\\\"ew\\\", padx=20, pady=(20, 10))\\n        \\n        # Title\\n        title_label = ctk.CTkLabel(\\n            header_frame,\\n            text=\\\"üöÄ Revolutionary Card Grader - Project Dashboard\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 24, \\\"bold\\\"),\\n            text_color=RevolutionaryTheme.NEON_CYAN\\n        )\\n        title_label.pack(pady=20)\\n        \\n        # Subtitle\\n        subtitle_label = ctk.CTkLabel(\\n            header_frame,\\n            text=\\\"Select an existing project or create a new one to begin\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 14),\\n            text_color=RevolutionaryTheme.GHOST_WHITE\\n        )\\n        subtitle_label.pack(pady=(0, 20))\\n        \\n        # Main content area\\n        content_frame = ctk.CTkFrame(self, fg_color=\\\"transparent\\\")\\n        content_frame.grid(row=1, column=0, sticky=\\\"nsew\\\", padx=20, pady=(0, 20))\\n        content_frame.grid_columnconfigure(0, weight=2)\\n        content_frame.grid_columnconfigure(1, weight=1)\\n        content_frame.grid_rowconfigure(0, weight=1)\\n        \\n        # Projects list\\n        self.setup_projects_list(content_frame)\\n        \\n        # Controls panel\\n        self.setup_controls_panel(content_frame)\\n    \\n    def setup_projects_list(self, parent):\\n        \\\"\\\"\\\"Setup the projects list\\\"\\\"\\\"\\n        \\n        # Projects frame\\n        projects_frame = ctk.CTkFrame(parent, fg_color=RevolutionaryTheme.NEURAL_GRAY, corner_radius=10)\\n        projects_frame.grid(row=0, column=0, sticky=\\\"nsew\\\", padx=(0, 10))\\n        projects_frame.grid_columnconfigure(0, weight=1)\\n        projects_frame.grid_rowconfigure(1, weight=1)\\n        \\n        # Projects header\\n        projects_header = ctk.CTkLabel(\\n            projects_frame,\\n            text=\\\"üìÅ Available Projects\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 18, \\\"bold\\\"),\\n            text_color=RevolutionaryTheme.GHOST_WHITE\\n        )\\n        projects_header.grid(row=0, column=0, pady=(20, 10))\\n        \\n        # Projects scrollable frame\\n        self.projects_scroll = ctk.CTkScrollableFrame(\\n            projects_frame,\\n            fg_color=RevolutionaryTheme.QUANTUM_DARK,\\n            corner_radius=8\\n        )\\n        self.projects_scroll.grid(row=1, column=0, sticky=\\\"nsew\\\", padx=20, pady=(0, 20))\\n        self.projects_scroll.grid_columnconfigure(0, weight=1)\\n    \\n    def setup_controls_panel(self, parent):\\n        \\\"\\\"\\\"Setup the controls panel\\\"\\\"\\\"\\n        \\n        # Controls frame\\n        controls_frame = ctk.CTkFrame(parent, fg_color=RevolutionaryTheme.NEURAL_GRAY, corner_radius=10)\\n        controls_frame.grid(row=0, column=1, sticky=\\\"nsew\\\")\\n        \\n        # Controls header\\n        controls_header = ctk.CTkLabel(\\n            controls_frame,\\n            text=\\\"üéØ Project Actions\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 18, \\\"bold\\\"),\\n            text_color=RevolutionaryTheme.GHOST_WHITE\\n        )\\n        controls_header.pack(pady=(20, 20))\\n        \\n        # Create new project button\\n        self.create_btn = ctk.CTkButton(\\n            controls_frame,\\n            text=\\\"‚ûï Create New Project\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 14, \\\"bold\\\"),\\n            width=200,\\n            height=50,\\n            fg_color=RevolutionaryTheme.NEON_CYAN,\\n            hover_color=RevolutionaryTheme.PLASMA_BLUE,\\n            command=self.create_new_project\\n        )\\n        self.create_btn.pack(pady=10, padx=20)\\n        \\n        # Load project button\\n        self.load_btn = ctk.CTkButton(\\n            controls_frame,\\n            text=\\\"üìÇ Load Selected\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 14, \\\"bold\\\"),\\n            width=200,\\n            height=50,\\n            fg_color=RevolutionaryTheme.PLASMA_BLUE,\\n            hover_color=RevolutionaryTheme.NEON_CYAN,\\n            command=self.load_selected_project,\\n            state=\\\"disabled\\\"\\n        )\\n        self.load_btn.pack(pady=10, padx=20)\\n        \\n        # Refresh button\\n        self.refresh_btn = ctk.CTkButton(\\n            controls_frame,\\n            text=\\\"üîÑ Refresh\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 12),\\n            width=200,\\n            height=35,\\n            fg_color=RevolutionaryTheme.NEURAL_GRAY,\\n            hover_color=RevolutionaryTheme.PLASMA_BLUE,\\n            command=self.refresh_projects\\n        )\\n        self.refresh_btn.pack(pady=10, padx=20)\\n        \\n        # Delete button\\n        self.delete_btn = ctk.CTkButton(\\n            controls_frame,\\n            text=\\\"üóëÔ∏è Delete Selected\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 12),\\n            width=200,\\n            height=35,\\n            fg_color=\\\"#8B0000\\\",\\n            hover_color=\\\"#660000\\\",\\n            command=self.delete_selected_project,\\n            state=\\\"disabled\\\"\\n        )\\n        self.delete_btn.pack(pady=10, padx=20)\\n        \\n        # Export button\\n        self.export_btn = ctk.CTkButton(\\n            controls_frame,\\n            text=\\\"üì§ Export Selected\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 12),\\n            width=200,\\n            height=35,\\n            fg_color=RevolutionaryTheme.NEURAL_GRAY,\\n            hover_color=RevolutionaryTheme.PLASMA_BLUE,\\n            command=self.export_selected_project,\\n            state=\\\"disabled\\\"\\n        )\\n        self.export_btn.pack(pady=10, padx=20)\\n        \\n        # Project info\\n        info_frame = ctk.CTkFrame(controls_frame, fg_color=RevolutionaryTheme.QUANTUM_DARK, corner_radius=8)\\n        info_frame.pack(fill=\\\"x\\\", padx=20, pady=20)\\n        \\n        info_header = ctk.CTkLabel(\\n            info_frame,\\n            text=\\\"üìä Project Info\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 14, \\\"bold\\\"),\\n            text_color=RevolutionaryTheme.GHOST_WHITE\\n        )\\n        info_header.pack(pady=(10, 5))\\n        \\n        self.info_label = ctk.CTkLabel(\\n            info_frame,\\n            text=\\\"Select a project to view details\\\",\\n            font=(RevolutionaryTheme.FONT_FAMILY, 11),\\n            text_color=RevolutionaryTheme.GHOST_WHITE,\\n            wraplength=180\\n        )\\n        self.info_label.pack(pady=(0, 10), padx=10)\\n    \\n    def refresh_projects(self):\\n        \\\"\\\"\\\"Refresh the projects list\\\"\\\"\\\"\\n        try:\\n            # Clear existing projects\\n            for widget in self.projects_scroll.winfo_children():\\n                widget.destroy()\\n            \\n            # Discover projects\\n            projects = self.project_manager.discover_existing_projects()\\n            \\n            if not projects:\\n                # No projects found\\n                no_projects_label = ctk.CTkLabel(\\n                    self.projects_scroll,\\n                    text=\\\"No projects found.\\\\nCreate a new project to get started!\\\",\\n                    font=(RevolutionaryTheme.FONT_FAMILY, 14),\\n                    text_color=RevolutionaryTheme.GHOST_WHITE\\n                )\\n                no_projects_label.pack(pady=50)\\n            else:\\n                # Add project items\\n                for i, project in enumerate(projects):\\n                    self.add_project_item(project, i)\\n            \\n            print(f\\\"üîÑ Refreshed projects list: {len(projects)} projects found\\\")\\n            \\n        except Exception as e:\\n            print(f\\\"‚ùå Error refreshing projects: {e}\\\")\\n            messagebox.showerror(\\\"Error\\\", f\\\"Failed to refresh projects:\\\\n{str(e)}\\\")\\n    \\n    def add_project_item(self, project: dict, index: int):\\n        \\\"\\\"\\\"Add a project item to the list\\\"\\\"\\\"\\n        \\n        # Project item frame\\n        item_frame = ctk.CTkFrame(\\n            self.projects_scroll,\\n  