  print(f\\\"üìê Creating layout: {self.current_columns} columns\\\")\\n        \\n        # Clear existing widgets\\n        for widget in self.scrollable_frame.winfo_children():\\n            widget.destroy()\\n        \\n        # Create grid layout\\n        for i, item in enumerate(self.items):\\n            row = i // self.current_columns\\n            col = i % self.current_columns\\n            \\n            # Create widget frame\\n            widget = ctk.CTkFrame(\\n                self.scrollable_frame,\\n                width=self.item_width,\\n                height=self.item_height,\\n                fg_color=RevolutionaryTheme.NEURAL_GRAY,\\n                border_width=1,\\n                border_color=RevolutionaryTheme.NEURAL_GRAY\\n            )\\n            widget.grid(row=row, column=col, padx=self.padding, pady=self.padding, sticky=\\\"nsew\\\")\\n            widget.grid_propagate(False)\\n            \\n            # Store widget reference\\n            item.widget = widget\\n            \\n            # Bind events\\n            widget.bind(\\\"<Button-1>\\\", lambda e, i=item: self._on_item_click(i))\\n            widget.bind(\\\"<Double-Button-1>\\\", lambda e, i=item: self._on_item_double_click(i))\\n            self._bind_mousewheel_recursive(widget)\\n            \\n            # Show loading or loaded content\\n            if item.loaded and item.thumbnail:\\n                self._update_item_display(item)\\n            else:\\n                # Show loading placeholder\\n                placeholder = ctk.CTkLabel(\\n                    widget,\\n                    text=\\\"üì∑\\\\nLoading...\\\",\\n                    font=(RevolutionaryTheme.FONT_FAMILY, 10),\\n                    text_color=RevolutionaryTheme.GHOST_WHITE\\n                )\\n                placeholder.pack(expand=True)\\n                self._bind_mousewheel_recursive(placeholder)\\n        \\n        # Configure column weights for centering\\n        for col 