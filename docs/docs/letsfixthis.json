emon=True).start()\\n        \\n        # Calculate initial layout\\n        self._calculate_layout()\\n        \\n    def _load_all_thumbnails(self):\\n        \\\"\\\"\\\"Load all thumbnails in background thread\\\"\\\"\\\"\\n        for item in self.items:\\n            try:\\n                # Load and resize image\\n                img = Image.open(item.path)\\n                \\n                # Calculate size maintaining aspect ratio\\n                img_ratio = img.width / img.height\\n                target_ratio = 104 / 110\\n                \\n                if img_ratio > target_ratio:\\n                    size = (104, int(104 / img_ratio))\\n                else:\\n                    size = (int(110 * img_ratio), 110)\\n                \\n                # Resize with high quality\\n                img = img.resize(size, Image.Resampling.LANCZOS)\\n                \\n                # Create CTkImage\\n                item.thumbnail = ctk.CTkImage(light_image=img, size=size)\\n                item.loaded = True\\n                \\n                # Update UI in main thread\\n                self.after(0, lambda i=item: self._update_item_display(i))\\n                \\n            except Exception as e:\\n                print(f\\\"Error loading {item.path}: {e}\\\")\\n                \\n        # Mark loading complete\\n        self.loading_complete = True\\n        self.after(0, lambda: print(\\\"‚úÖ All images loaded!\\\"))\\n    \\n    def _update_item_display(self, item: SimpleGridItem):\\n        \\\"\\\"\\\"Update item display with loaded thumbnail\\\"\\\"\\\"\\n        if item.widget and item.widget.winfo_exists():\\n            # Clear loading placeholder\\n            for child in list(item.widget.winfo_children()):\\n                child.destroy()\\n            \\n            if item.thumbnail:\\n                # Add image\\n                img_label = ctk.CTkLabel(\\n                    item.widget,\\n                    text=\\\"\\\